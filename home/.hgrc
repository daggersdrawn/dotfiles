[ui]
merge = changes
username = Thomas Schreiber <tom@insatsu.us>
editor = emacs

[merge-tools]
p4.priority = 100
p4.premerge = True
p4.executable = /Applications/p4merge.app/Contents/MacOS/p4merge
p4.gui = True
p4.args = $base $local $other $output

[extensions]
color =
pager =
parentrevspec =
rebase =
hgk =
hgext.record =
hgext.extdiff =
hgext.graphlog =
hgext.highlight =
hgext.mq =
hgshelve = ~/.hgextensions/hgshelve.py
hgflow = ~/.hgextensions/hgflow-0.2.py
transplant=

[bookmarks]
track.current = True

[pager]
pager = less -R
attend = help log glog diff cat heads incoming outgoing annotate

[hgk]
path=~/.hgextensions/bin/hgk

[extdiff]
cmd.p4diff = p4merge

